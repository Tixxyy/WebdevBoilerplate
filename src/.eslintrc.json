{
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "react-hooks"
    ],
    "rules": {
        "react-hooks/rules-of-hooks": "error", 
        "react-hooks/exhaustive-deps": "warn" 
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],

    "env": {
        "browser": true,
        "es6": true
    }
}
